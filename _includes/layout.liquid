<!DOCTYPE html>
<html
  lang="en"
  class="theme"
  x-data="{
    selectedTech: [],
    theme: 'default',
    themes: [
      'blue', 
      'red',
      'fugio',
      'yellow',
      'pink',
      'beach',
      'dark',
      'franz',
    ],
    handleClick() {
      const newTheme = this.pickRandomTheme();
      if (newTheme) {
        this.theme = newTheme;
        localStorage.setItem('theme', newTheme)
      }
    },
    pickRandomTheme() {
      const currentThemeIndex = this.themes.indexOf(this.theme);
      let newIndex = Math.floor(Math.random() * this.themes.length);
      
      if (currentThemeIndex === newIndex) {
        this.pickRandomTheme();
        return false;
      }
      return this.themes[newIndex];
    },
    init() {
      // NOTE: Remove the theme classes that were added before Alpine loaded, Alpine will add them back
      const classes = [...document.documentElement.classList].filter((c) => !c.startsWith('theme--'));
      document.documentElement.classList = classes;

      const theme = localStorage.getItem('theme');
      if (theme) { this.theme = theme; }
    }
  }"
  x-bind:class="{ [`theme--${theme}`]: true }"
>
